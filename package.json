{
	"name": "my-app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev:css": "unocss 'app/**/*.tsx' --out-file=uno.css --watch",
		"dev:next": "next dev",
		"dev": "run-p dev:*",
		"css": "unocss 'app/**/*.tsx' --out-file=uno.css",
		"build": "npm run css && next build",
		"start": "next start",
		"lint": "next lint",
		"format": "prettier --write --ignore-path .gitignore ."
	},
	"dependencies": {
		"@types/node": "20.2.5",
		"@types/react": "18.2.7",
		"@types/react-dom": "18.2.4",
		"eslint-config-next": "13.4.4",
		"next": "13.4.4",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"@unocss/cli": "^0.52.4",
		"@unocss/preset-rem-to-px": "^0.52.4",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-unicorn": "^47.0.0",
		"npm-run-all": "^4.1.5",
		"prettier": "2.8.8",
		"typescript": "^5.0.4",
		"unocss": "^0.52.4"
	}
}
